{
    "workbench.sideBar.location": "right",
    "workbench.iconTheme": "vscode-icons",
    "vscord.status.details.text.debugging": "Debug: {file_name}{file_extension}",
    "vscord.status.details.text.editing": "Workspace:  {workspace}",
    "vscord.status.details.text.idle": "Away From Keyboard.",
    "vscord.status.details.text.notInFile": "",
    "vscord.status.details.text.noWorkSpaceText": "",
    "vscord.status.details.text.viewing": "Workspace: {workspace}",
    "vscord.status.state.text.debugging": "Workspace: {workspace}",
    "vscord.status.state.text.editing": "Edit: {file_name}{file_extension}",
    "vscord.status.state.text.noWorkspaceFound": "",
    "vscord.status.state.text.viewing": "View: {file_name}{file_extension}",
    "vscord.status.image.large.idle.text": "AFK",
    "vscord.status.image.small.idle.text": "",
    "vscord.status.image.small.notInFile.text": "",
    "editor.fontSize": 14.4,
    "editor.fontFamily": "monaspace argon, SF Mono Medium, cascadia code, JetBrains Mono, consolas, ibm plex mono", 
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment"
                ],
                "settings": {
                    "foreground": "#b6b6b6"
                }
            },
            {
                "scope": [
                    "constant",
                    "storage.modifier",
                    "storage.type.class.js",
                    "keyword",
                    "keyword.function",
                    "keyword.constructor"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "support.function.console",
                    "meta.function-call.js",
                    "meta.method-call.js"
                ],
                "settings": {
                    "fontStyle": ""  // Menghapus italic dari method dan function call
                }
            },
            {
                "scope": [
                    "entity.name.function",           
                    "entity.name.class",              
                    "meta.parameters",               
                    "variable.parameter.constructor" 
                ],
                "settings": {
                    "fontStyle": "italic"  // Italic untuk parameter function dan constructor
                }
            },
            {
                "scope": [
                    "punctuation.definition.parameters",
                    "punctuation.section.parameters"
                ],
                "settings": {
                    "fontStyle": ""  // Tanda kurung tidak italic
                }
            }
        ]
    },
    "editor.wordWrap": "on",
    "editor.cursorStyle": "line",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "workbench.colorCustomizations": {
        "activityBar.foreground": "#ffffff"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "all",
    "prettier.vueIndentScriptAndStyle": true,
    "vscord.app.name": "Visual Studio Code",
    "oneDarkPro.vivid": true,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "discord.lowerDetailsNoWorkspaceFound": "",
    "discord.lowerDetailsIdling": "",
    "discord.largeImageIdling": "",
    "editor.tabSize": 4,
    "editor.tabCompletion": "on",
    "indentRainbow.indicatorStyle": "classic",
    "glassit.alpha": 255,
    "workbench.editor.editorActionsLocation": "titleBar",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.tabSizingFixedMaxWidth": 110,
    "window.density.editorTabHeight": "compact",
    "prettier.withNodeModules": true,
    "prettier.useTabs": true,
    "prettier.semi": false,
    "terminal.integrated.fontFamily": "sf mono",
    "terminal.integrated.fontWeightBold": "normal",
    "editor.fontWeight": "normal",
    "editor.fontLigatures": false,
    "editor.experimentalWhitespaceRendering": "font",
    "vscode-pets.throwBallWithMouse": true,
    "vscode-pets.theme": "castle",
    "indentRainbow.lightIndicatorStyleLineWidth": 1,
    "indentRainbow.ignoreLinePatterns": [
        "/[ \t]* [*]/g",
        "/[ \t]+[/]{2}/g"
    ],
    "editor.wrappingIndent": "deepIndent",
    "vscord.status.image.large.editing.text": "Editing the {LANG} file",
    "vscord.status.image.large.viewing.text": "Viewing the {LANG} file",
    "vscord.behaviour.statusBarAlignment": "Right",
    "terminal.integrated.env.windows": {},
    "console-ninja.featureSet": "Community",
    "workbench.colorTheme": "One Dark Pro Flat",
    "files.autoSave": "afterDelay"
}
